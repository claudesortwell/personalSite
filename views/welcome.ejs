<script>
    $(document).ready(function () {
        $(".projectSection").toggleClass(function (index, currentclass) {
            if (index % 2 == 0) {
                return "projectSectionBgEven";
            } else {
                return "projectSectionBgOdd";
            }
        });
    });
</script>