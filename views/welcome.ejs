<div id="projectPart" class="projectPart">
    <p>Various projects I have built</p>
    <H1 style="font-size: 58px;font-weight: 600;margin-top: 5px;margin-bottom: 0;">Projects</H1><br />
    <% if(projects != null) { %> <% var index = 0 %> <% projects.forEach(function(projects) { %>
    <div id="projectCard" class="projectCard">
        <img src="/img/<%= projects.projectName %>.jpg" alt=""">
    <div class=" projectCardTextArea">
        <h2><%= projects.projectName %></h2>
        <p style="color: #6d6d6deb;font-weight: 600;font-family: 'Montserrat', sans-serif;">
            <%= projects.projectPlatform %></p>
        <h6>Built with <%= projects.projectStack %></h6>
        <button onClick="document.getElementById('<%= projects.projectName %>').scrollIntoView();">
            <i class="mdi mdi-arrow-down-bold"
                style="font-size: 20px;top: 2px;margin-right: 5px;position: relative;"></i>
            <span style="top: -1.5px;position: relative;">View More</span>
        </button>
    </div>
</div>
<% index ++ %> <% }); %> <% } %>
</div>

<% if(projects != null) { %> <% var index = 0 %> <% projects.forEach(function(projects) { %>
<div id="<%= projects.projectName %>" class="projectSection">
    <div class="projectSectionText">
        <h6 class="projectTextParts"><%= projects.projectStartDate %></h6>
        <img src="/img/logo/<%= projects.projectName %>.png" alt="">
        <p class="projectTextParts"><%= projects.projectPlatform %></p>
        <h3><%= projects.projectName %></h3>
        <p class="projectTextParts"><%= projects.projectDesc %></p>
        <a href="<%= projects.projectGithubLink %>" target="_blank"><i class="mdi mdi-github-circle"></i> Open
            Github</a>
        <% if(projects.projectLiveLink != 'null') { %><a href="<%= projects.projectLiveLink %>" target="_blank"><i
                class="mdi mdi-open-in-new"></i> View Live
            Version</a>
        <% } %>
    </div>

    <img class="projectDemoPic" src="/img/<%= projects.projectName %>.jpg" alt="">
</div>
<% index ++ %> <% }); %> <% } %>

